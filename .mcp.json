{
  "mcpServers": {
    "supabase": {
      "type": "http",
      "url": "https://mcp.supabase.com/mcp"
    },
    "transactoid": {
      "type": "stdio",
      "command": "uv",
      "args": ["run", "python", "-m", "transactoid.ui.mcp.server"]
    },
    "playwriter": {
      "command": "npx",
      "args": [
        "playwriter"
      ]
    },
    "promptorium-python": {
      "type": "stdio",
      "command": "uv",
      "args": ["run", "prompts-mcp"]
    }
  }
}
